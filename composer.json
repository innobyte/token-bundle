{
    "name": "innobyte/token-bundle",
    "type": "symfony-bundle",
    "keywords": ["token", "hash", "generator"],
    "homepage": "http://www.innobyte.com/",
    "description": "A Symfony2 Bundle implementing an access-based token system, with generation, validation and consumption mechanisms.",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    },
    "require": {
        "php": ">5.3.3",
        "symfony/symfony": ">=2.3",
        "twig/twig": ">=1.10",
        "doctrine/doctrine-bundle": ">=1.1"
    },
    "autoload": {
        "psr-0": { "Innobyte\\TokenBundle": "" }
    },
    "target-dir": "Innobyte/TokenBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sorin Dumitrescu",
            "email": "sorin.dumitrescu@innobyte.com"
        }
    ],
    "version": "1.1.0",
    "minimum-stability": "stable"
}
