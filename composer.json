{
    "name": "innobyte/token-bundle",
    "type": "symfony-bundle",
    "keywords": ["token", "hash", "generator"],
    "homepage": "http://www.innobyte.com/",
    "description": "A Symfony3 Bundle implementing an access-based token system, with generation, validation and consumption mechanisms.",
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "symfony/symfony": "3.1.*",
        "twig/twig": "~1.28|~2.0",
        "doctrine/doctrine-bundle": "~1.4"
    },
    "autoload": {
        "psr-0": { "Innobyte\\TokenBundle": "" }
    },
    "target-dir": "Innobyte/TokenBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sorin Dumitrescu",
            "email": "sorin.dumitrescu@innobyte.com"
        }
    ],
    "version": "2.0.2",
    "minimum-stability": "dev"
}
